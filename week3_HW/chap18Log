[?1000h[?2004h[?1049h[22;0;0t[?1h=[?2004h[1;63r[?12h[?12l[22;2t[22;1t[27m[23m[29m[m[H[2J[?25l[63;1H"ex6.c" 51L, 936C[2;1Hâ–½[6n[2;1H  [1;1H[>c]10;?]11;?[1;1H[34m//ex [m[31m6[m[34m, pg [m[31m525.[m[34m Write a function to perform a bitwise shift on an unsigned short: unsigned short swap_bytes(unsigned short i);
//
//
//[m
[35m#include [m[31m<stdio.h>[m
[35m#include [m[31m<stdlib.h>[m


[35m#define MAX_SHORT = [m[31m65535[m

[32munsigned[m [32mshort[m swap_bytes([32munsigned[m [32mshort[m i){[13;9H[32munsigned[m [32mshort[m left_shift;[14;9H[32munsigned[m [32mshort[m right_shift;[16;9Hleft_shift = i<<[31m8[m;[17;9Hright_shift = i>>[31m8[m;[20;9H[34m//printf([m[31m"left: %hx [m[35m\n[m[31m"[m[34m, left_shift);[21;9H//printf([m[31m"right: %hx [m[35m\n[m[31m"[m[34m, right_shift);[22;9H//printf([m[31m"sum: %hx [m[35m\n[m[31m"[m[34m, left_shift+right_shift);[m[24;9H[33mreturn[m (left_shift + right_shift);
