[32munsigned[m [32mshort[m swap_bytes([32munsigned[m [32mshort[m i){[13;9H[32munsigned[m [32mshort[m left_shift;[14;9H[32munsigned[m [32mshort[m right_shift;[16;9Hleft_shift = i<<[31m8[m;[17;9Hright_shift = i>>[31m8[m;[20;9Hprintf([31m"left: [m[35m%hx[m[31m [m[35m\n[m[31m"[m, left_shift);[21;9Hprintf([31m"right: [m[35m%hx[m[31m [m[35m\n[m[31m"[m, right_shift);[22;9Hprintf([31m"sum: [m[35m%hx[m[31m [m[35m\n[m[31m"[m, left_shift+right_shift);[24;9H[33mreturn[m [31m0[m ;
