bool equal_array(int counts1[26], int counts2[26]){

        int i;
        
        for(i = 0;i<26;i++){
                if(counts1[i] != counts2[i]){
                        //printf([m[1Cdifference found at index %d[1C[31m, i);
                        return false;
                }
        }[m[24;66H10-17[11;17H[?25h[?25l[35m\[m[24;67H1-18[11;18H[?25h[?25l[35mn[m[24;67H2-19[11;19H[?25h[?25l[31m"[m
}

