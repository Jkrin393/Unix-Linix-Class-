[?1000h[?2004h[?1049h[22;0;0t[?1h=[?2004h[1;63r[?12h[?12l[22;2t[22;1t[27m[23m[29m[m[H[2J[?25l[63;1H"anagram.c" 69L, 1273C[2;1Hâ–½[6n[2;1H  [1;1H[>c]10;?]11;?[1;1H[32mint[m main(){[3;9H[32mchar[m word1[[31m20[m];[4;9H[32mchar[m word2[[31m20[m];[6;9H[32mbool[m isAnagram = [31mtrue[m;[8;9Hprintf([31m"Please enter the first word: "[m);[9;9Hscanf([31m"[m[35m%s[m[31m"[m, word1);[11;9Hprintf([31m"Please enter the second  word: "[m);[12;9Hscanf([31m"[m[35m%s[m[31m"[m, word2);[14;9H[32mint[m i;[15;9H[32mint[m len1,len2;[17;9H[34m//test for same length[m[18;9Hlen1 = strlen(word1);[19;9Hlen2 = strlen(word2);[20;9H[33mif[m(len1 != len2){[21;17HisAnagram = [31mfalse[m;[22;9H}[24;9H[32mint[m letter_count[[31m26[m] = {[31m0[m};[25;9H[34m//array of [m[31m26[m[34m to count each letter. If the charachter is alphabetical, change it to lower case, and increment the matching index in letter count by subtracting the ascii value if a.[m[26;9H[33mfor[m(i=[31m0[m;i<len1;i++){[27;17H[33mif[m(isalpha(word1[i])){[28;25Hword1[i] = tolower(word1[i]);[29;25Hletter_count[word1[i] - [31m'a'[m]++;[30;17H}[31;9H}[33;9H[34m// similar to the first word, but we decrement the count as we see letters[m[34;9H[33mfor[m(i=[31m0[m;i<len2;i++){[35;17H[33mif[m(isalpha(word2[i])){[36;25Hword2[i] = tolower(word2[i]);[37;25Hletter_count[word2[i] - [31m'a'[m]--;[38;17H}[39;9H}[42;9H[33mfor[m(i=[31m0[m;i<[31m26[m;i++){[43;17H[33mif[m(letter_count[i] !=[31m0[m){[44;25HisAnagram = [31mfalse[m;[45;25H[33mbreak[m;[46;17H}[47;9H}[50;9H[33mif[m(isAnagram == [31mtrue[m){[51;17Hprintf([31m"the words are anagrams!"[m);[52;9H}[53;9H[33melse[m{[54;17Hprintf([31m"the words are NOT anagrams!"[m);[55;9H}[57;9H[33mreturn[m [31m0[m;
