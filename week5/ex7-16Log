time [m[47;87H[K[47;87H49,1-8[8C78%[1;12H[?25h[?25l[1;46r[1;1H[L[1;47r[1;1H[33m 48 [m[16C[36m//start timer[m[47;87H[K[47;87H48,1-8[8C77%[1;12H[?25h[?25l[1;46r[1;1H[L[1;47r[1;1H[33m 47 [m[16Crefresh();[36m//push buffer to screen, refresh buffer[m[47;87H[K[47;87H47,1-8[8C75%[1;12H[?25h[?25l[1;46r[1;1H[2L[1;47r[1;1H[33m 46 [m[16Cmvprintw([33m0[m,[33m0[m,[33m"The numbder is: [m[35m%d\n[m[33m"[m, game_num);[36m//move the rand num to the screen buff[m[2;1H[33m    [m[36mfer[m[47;87H[K[47;87H46,1-8[8C72%[1;12H[?25h[?25l[1;46r[1;1H[L[1;47r[1;1H[33m 45 [m[16C[32mint[m game_num = rand()%[33m10[m;[47;87H[K[47;87H45,1-8[8C70%[1;12H[?25h[?25l[1;46r[1;1H[L[1;47r[1;1H[33m 44 [m[47;87H[K[47;87H44,0-1[8C69%[1;5H[?25h[?25l[1;46r[1;1H[2L[1;47r[1;1H[33m 43 [m[16Cnanosleep(&wait_time, [33mNULL[m ); [36m// nanosleep takes a timepec struct as the first argumm[m[2;1H[33m    [m[36ments, and a handler interrupt as the second)[m[47;87H[K[47;87H43,1-8[8C66%[1;12H[?25h[?25l[1;46r[1;1H[L[1;47r[1;1H[33m 42 [m[47;87H[K[47;87H42,1-8[8C65%[1;12H[?25h[?25l[1;46r[1;1H[L[1;47r[1;1H[33m 41 [m[47;87H[K[47;87H41,1-8[8C63%[1;12H[?25h[?25l[1;46r[1;1H[L[1;47r[1;1H[33m 40 [m[16Cwait_time.tv_nsec = rand() % [33m500000000[m;[47;87H[K[47;87H40,1-8[8C61%[1;12H[?25h[?25l[1;46r[1;1H[L[1;47r[1;1H[33m 39 [m[16Cwait_time.tv_sec = rand() % [33m10[m;[47;87H[K[47;87H39,1-8[8C60%[1;12H[?25h[?25l[1;46r[1;1H[L[1;47r[1;1H[33m 38 [m[16C[32mstruct[m timespec wait_time;[47;87H[K[47;87H38,1-8[8C58%[1;12H[?25h[?25l[1;46r[1;1H[L[1;47r[1;1H[33m 37 [m[16C[36m//wait for a random time up to 10 seconds[m[47;87H[K[47;87H37,1-8[8C57%[1;12H[?25h[?25l[1;46r[1;1H[L[1;47r[1;1H[33m 36 [m[16Crefresh();[47;87H[K[47;87H36,1-8[8C55%[1;12H[?25h[?25l[1;46r[1;1H[L[1;47r[1;1H[33m 35 [m[16Cclear()[47;87H[