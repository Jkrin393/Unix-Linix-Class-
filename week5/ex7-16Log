[32mint[m main(){[18;9Hsrand(time([33mNULL[m)); [36m//set seed for random time[20;9H//initialize screem, turn off echo, character buffering, and delay time before process continues[m[21;9Hinitscr();[22;9Hcbreak();[23;9Hnoecho();[24;9Hnodelay(stdscr, TRUE);[26;9H[36m//wait for a random time up to [m[33m10[m[36m seconds[m[27;9H[32mint[m wait_time = rand() % [33m10[m;[28;9Husleep(wait_time); [36m// usleep is a depricated but more accurate sleep timer. better for measuring response timm[29;1He[m[31;9H[32mint[m game_num = rand()%[33m10[m;[32;9Hmvprintw([33m0[m,[33m0[m,[33m"The numbder is: [m[35m%d\n[m[33m"[m, game_num);[36m//move the rand num to the screen buffer[m[33;9Hrefresh();[36m//push buffer to screen, refresh buffer[m[37;9Hclear();[38;9Hendwin();[42;1H}[47;1H[34m~                                                                                                                    [48;1H~                                                                                                                    [49;1H~                                                                                                                    [50;1H~                                                                                                                    [51;1H~                                                                                                                    [52;1H~                                                                                                                    [53;1H~                                                                                                                    [54;1H~                                                                                                                    [55;1H~                                                                                                                    [56;1H~                                                      