[32mint[m main(){[25;9H[32mint[m input_pid_num;[26;9H[32mint[m i;[27;9Hprintf([33m"Pick a number of child processes: "[m);[28;9Hscanf([33m"[m[35m%d[m[33m"[m, &input_pid_num);[30;9H[36m//fork the current process to create child processes [m[31;9Hpid_t pid;[32;9H[33mfor[m(i = [33m0[m; i<input_pid_num; i++){[33;17Hpid = fork();[36m//fork returns a child PID of 0 on successful fork [m[34;17H[33mif[m(pid == [33m0[m){[35;25Hprintf([33m"fork successful"[m);[36;25Hsleep([33m5[m);[37;17H}[38;17H[33melse[m [33mif[m(pid == [33m0[m){[39;25Hperror([33m"Fork error"[m);[40;25Hexit([33m1[m);[41;17H}[42;9H}[36m//for loop     [m[44;9H[33mwhile[m(c_count < input_pid_num){[45;17Hprintf([33m"waiting for all my children..sigh..."[m);[46;17Hsleep([33m1[m);[47;9H}[49;9Hprintf([33m"All children fiinshed, exiting now"[m);[54;1H}[36m//end main[m
[34m~                                                                                                                    [56;1H~                                                                                                                    [57;1H~                                                                                                                    [58;1H~                                                                                                                    [59;1H~                                                                                                                    [60;1H~                                                                                                                    [61;1H~                                                                                                                    [m[62;100H1,1[11CAll[1;1H[?25h[?25l[62;100H2[2;1H[?25h[?25l[62;100H3[3;1H[?25h[?25l[62;100H4[4;1H[?25h[?25l[62;100H5[5;1H[?25h[?25l[62;100H6[6;1H[?25h[?25l[62;100H7,0-1[7;1H[?25h[?25l[62;100H8,1  [8;1H[?25h[?25l[62;100H9,0-1[9;1H[?25h[?25l[62;100H10,1 [10;1H[?25h[?25l[62;101H1,1-8[11;8H[?25h[?25l[62;101H2,0-1[12;1H[?25h[?25l[62;101H3,1-8[13;8H[?25h[?25l[62;101H4[14;8H[?25h[?25l[62;101H5[15;8H[?25h[?25l[62;101H6[16;8H[?25h[?25l[62;101H7[17;8H[?25h[?25l[62;101H8[18;8H[?25h[?25l[62;101H9[19;8H[?25h[?25l[62;100H20[20;8H[?25h[?25l[62;101H1[21;8H[?25h[?25l[10;33H[46m{[22;1H}[m[62;101H2,1  [22;1H[?25h[?25l[10;33H{[22;1H}[62;101H3,0-1[23;1H[?25h[?25l[62;101H4,1  [24;1H[?25h[?25l[62;101H5,1-8[25;8H[?25h[?25l[62;101H6[26;8H[?25h[?25l[62;101H7[27;8H[?25h[?25l[62;101H8[28;8H[?25h[?25l[62;101H9,0-1[29;1H[?25h[?25l[62;100H30,1-8[30;8H[?25h[?25l[62;101H1[31;8H[?25h[?25l[62;101H2[32;8H[?25h[?25l[62;101H3[33;8H[?25h[?25l[62;101H4[34;8H[?25h[?25l[62;101H5[35;8H[?25h[?25l[62;101H6[36;8H[