[32mvoid[m child_handler([32mint[m sig_code){[11;9H[32mint[m status;[36m//store value returned by wait()[m[12;9Hpid_t child_pid;[14;9H[36m//wait for child process. wait will return -1 on error(no message) or PID on success[m[15;9Hchild_pid = wait(&status);[17;9H[33mif[m(status==-[33m1[m){[18;17Hprintf([33m"error in wait function, child returned -1"[m);[19;17Hexit([33m1[m);[20;9H}[21;9Hc_count++;

