[32mint[m main(){[10;9H[32mchar[m input_string[[33m100[m];[11;9H[32mchar[m** argv = [33mNULL[m;[12;9H[32mint[m argc = [33m0[m;[13;9H[32mint[m len;[14;9H[32mchar[m* cp;[16;9Hprintf([33m"Enter a command >>"[m);[17;9Hfgets(input_string, [33msizeof[m(input_string), [33mstdin[m);[20;9H[36m//count num args, split words[m[21;9Hcp = input_string;[22;9H[32mchar[m* start;[24;9H[33mwhile[m(*cp != [35m'\0'[m){[25;17H[33mwhile[m(is_delim(*cp)){[26;25Hcp++;[27;17H}[28;17H[33mif[m(*cp == [35m'\0'[m)[29;25H[33mbreak[m;[31;17Hstart = cp;[32;17Hlen = [33m0[m;[34;17H[33mwhile[m(*cp != [35m'\0'[m && !(is_delim(*cp))){[35;25Hlen++;[36;25Hcp++;[37;17H}[39;17Hargv = realloc(argv, (argc+[33m1[m) * [33msizeof[m([32mchar[m*));[40;17Hargv[argc] = malloc((len+[33m1[m)*[33msizeof[m([32mchar[m));[41;17Hstrncpy(argv[argc] ,start, len);[43;17Hargc++;[44;9H}[45;9Hargv = realloc(argv, (argc+[33m1[m) * [33msizeof[m([32mchar[m*));[46;9Hargv[argc] = [33mNULL[m; [36m// terminate array[m[48;9Hexecvp(argv[[33m0[m],argv);[50;9H[33mfor[m([32mint[m i = [33m0[m;i<argc;i++){[51;17Hfree(argv[i]);[52;9H}[53;9Hfree(argv);[54;9H[33mreturn[m [33m0[m;
