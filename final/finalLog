[32mint[m main(){[27;9H[36m//set signal handler as the child_handler function[m[29;9Hsignal([33mSIGCHLD[m, child_handler);[31;9H[32mint[m input_pid_num;[32;9H[32mint[m i;[33;9Hprintf([33m"Pick a number of child processes: "[m);[34;9Hscanf([33m"[m[35m%d[m[33m"[m, &input_pid_num);[36;9H[36m//fork the current process to create child processes [m[37;9Hpid_t pid;[38;9H[33mfor[m(i = [33m0[m; i<input_pid_num; i++){[39;17Hpid = fork();[36m//fork returns a child PID of 0 on successful fork [m[40;17H[33mif[m(pid == [33m0[m){[41;25Hprintf([33m"fork successful[m[35m\n[m[33m"[m);[42;25Hsleep([33m5[m); [36m// child function is sleep()[m[43;25Hexit([33m0[m);[36m//end child process[m[44;17H}[45;17H[33melse[m [33mif[m(pid == -[33m1[m){[46;25Hperror([33m"Fork error"[m);[47;25Hexit([33m1[m);[48;17H}[49;9H}[36m//for loop     [m[52;9H[33mwhile[m(c_count < input_pid_num){[53;17Hprintf([33m"waiting for all my children..sigh...[m[35m\n[m[33m"[m);[54;17Hsleep([33m1[m);[55;9H}[57;9Hprintf([33m"All children fiinshed, exiting now[m[35m\n[m[33m"[m);[62;1H}[36m//end main[m[63;220H1,1[11CAll[1;1H[?25h[?25l[63;220H2[2;1H[?25h[?25l[63;220H3[3;1H[?25h[?25l[63;220H4[4;1H[?25h[?25l[63;220H5[5;1H[?25h[?25l[63;220H6[6;1H[?25h[?25l[63;220H7,0-1[7;1H[?25h[?25l[63;220H8,1  [8;1H[?25h[?25l[63;220H9,0-1[9;1H[?25h[?25l[63;220H10,1 [10;1H[?25h[?25l[63;221H1,1-8[11;8H[?25h[?25l[63;221H2[12;8H[?25h[?25l[63;221H3,0-1[13;1H[?25h[?25l[63;221H4,1-8[14;8H[?25h[?25l[63;221H5[15;8H[?25h[?25l[63;221H6[16;8H[?25h[?25l[63;221H7[17;8H[?25h[?25l[63;221H8[18;8H[?25h[?25l[63;221H9[19;8H[?25h[?25l[63;220H20[20;8H[?25h[?25l[63;221H1[21;8H[?25h[?25l[63;221H2[22;8H[?25h[?25l[10;33H[46m{[23;1H}[m[63;221H3,1  [23;1H[?25h[?25l[10;33H{[23;1H}[63;221H4,0-1[24;1H[?25h[?25l[63;221H5,1  [25;1H[?25h[?25l[63;221H6,0-1[26;1H[?25h[?25l[63;221H7,1-8[27;8H[?25h[?25l[63;221H8[28;8H[?25h[?25l[63;221H9[29;8H[?25h[?25l[63;220H30[30;8H[?25h[?25l[63;221H1[31;8H[?25h[?25l[63;221H2[32;8H[?25h[?25l[63;221H3[33;8H[?25h[?25l[63;221H4[34;8H[?25h[?25l[63;221H5,0-1[35;1H[?25h[?25l[63;221H6,1-8[36;8H[?25h[?25l[63;221H7[37;8H[?25h[?25l[63;221H8[38;8H[?25h[?25l[63;221H9[39;8H[?25h[?25l[63;220H40[40;8H[?25h[?25l[63;221H1[41;8H[?25h[?25l[63;221H2[42;8H[?25h[?25l[63;221H3[43;8H[?25h[?25l[63;221H4[44;8H[?25h[?25l[63;221H5[45;8H[?25h[?25l[63;221H6[46;8H[?25h[?25l[63;221H7[47;8H[?25h[?25l[63;221H8[48;8H[?25h[?25l[63;221H9[49;8H[?25h[?25l[63;220H50,0-1[50;1H[?25h[?25l[63;221H1[51;1H[?25h[?25l[63;221H2,1-8[52;8H[?25h[?25l[63;221H3[53;8H[?25h[?25l[63;221H4[54;8H[?25h[?25l[63;221H5[55;8H[?25h[?25l[63;221H6,0-1[56;1H[?25h[?25l[63;221H7,1-8[57;8H[?25h[?25l[63;221H8,0-1[58;1H[?25h[?25l[63;221H9,1-8[59;8H[?25h[?25l[63;220H60,0-1[60;1H[?25h[?25l[63;221H1[61;1H[?25h[?25l[25;11H[46m{[62;1H}[m[63;221H2,1  [62;1H[?25h[?25l[63;1H[K[63;1H:[?2004h[?25hw[?25l[?25hq[?25l[?25h[?25l[?2004l"sigchlds.c" 62L, 1220C written[23;2t[23;1t[22;2t[22;1t[23;2t[23;1t
