}


int main(){
        pid_t pid;
        int i;
        //create child procceses in a loop

        for(i=0;i<NUM_PROCESSES;i++){
                pid = fork();
                //check fork returned successfully              
                if(pid <0){
                        perror([m[1Cfork issue\n[1C[33m);
                        exit(1);
                }       
                //dynamically print process num 
                else if(pid == 0){
                        printf([m[1CI am child %d! \n[1C[33m, i);
                        break;
                }
        }
        
        //parent waits for all children to finish
        int child_count = 0;
        while(child_count<2){
                wait(NULL);
                i++;
        }

        return 0;
}[m[63;222H10-17[9;17H[?25h[?25l[33mi[m[63;223H1-18[9;18H[?25h[?25l[33m [m[63;223H0-17[9;17H[?25h[?25l         [9;25H[K[10;1H        
}


[32mint[m main(){
        pid_t pid;
        [32mint[m i;
        [36m//create child procceses in a loop[m

        [33mfor[m(i=[1C;i<NUM_PROCESSES;i++){
                pid = fork();
                [36m//check fork returned successfully              [m
                [33mif[m(pid <[1C){
                        perror([1C[33mfork issue[m[35m\n[m[1C);
                        exit([1C);
                }       
                [36m//dynamically print process num [m
                [33melse[m [33mif[m(pid == [1C){
                        printf([1C[33mI am child [m[35m%d[m[33m! [m[35m\n[m[1C, i);
                        [33mbreak[m;
                }
        }
        
        [36m//parent waits for all children to finish[m
        [32mint[m child_count = [1C;
        [33mwhile[m(child_count<[1C){
                wait([4C);
                i++;
        }

        [33mreturn[m [1C;
