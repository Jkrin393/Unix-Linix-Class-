void do_ls(char dirname[]){
        //list files in directory <dirname>
        DIR *dir_ptr; /*the directory*/
        struct dirent *direntp; //each entry. dirent has d_name, d_type, d_reclen
        
        if((dir_ptr = opendir(dirname))==NULL){
                fprintf(stderr, [m[1Cls1: cannot open %s\n[1C[31m, dirname);
                return;
        }
        
        while(( direntp=readdir(dir_ptr))!=NULL){
                if((direntp -> d_name[0])== '.'){
                        continue;
                }
                printf([m[1C%s\n[1C[31m, direntp->d_name);
        }
        closedir(dir_ptr);
        
        
}[m[63;223H10-17[11;17H[?25h[?25l[31mi[m[63;224H1-18[11;18H[?25h[?25l[11;17H[K[63;224H0-17[11;17H[?25h[?25l[31mA[m[63;224H1-18[11;18H[?25h[?25l[31mC[m[63;224H2-19[11;19H[?25h[?25l[31m:[m[63;224H3-20[11;20H[?25h[?25l[31m [m[63;224H4-21[11;21H[?25h[?25l[31m%[m[63;224H5-22[11;22H[?25h[?25l[35m%s[m[63;224H6-23[11;23H[?25h[?25l[31m%[m[11;22H[K[63;224H5-22[11;22H[?25h[?25l[35m%d[m[63;224H6-23[11;23H[?25h[?25l[31m,[m[63;224H7-24[11;24H[?25h[?25l[11;23H[K[63;224H6-23[11;23H[?25h[?25l[31m"[m
        [33mif[m(ac==[1C)[1m[37m[41m{[m
                do_ls([1C[31m.[m[1C);
        [1m[37m[41m}[m
        [33melse[m[1m[37m[41m{[m
