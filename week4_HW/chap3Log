[32mvoid[m do_ls([32mchar[m dirname[]){[23;9H[34m//list files in directory <dirname>[m[24;9H[32mDIR[m *dir_ptr; [34m/*the directory*/[m[25;9H[32mstruct[m dirent *direntp; [34m//each entry. dirent has d_name, d_type, d_reclen[m[27;9H[33mif[m((dir_ptr = opendir(dirname))==[31mNULL[m){[28;17Hfprintf([31mstderr[m, [31m"ls1: cannot open [m[35m%s\n[m[31m"[m, dirname);[29;9H}[30;9H[33melse[m{[31;17H[33mwhile[m(( direntp=readdir(dir_ptr))!=[31mNULL[m){[32;25Hprintf([31m"[m[35m%s\n[m[31m"[m, direntp->d_name);[33;17H}[34;9H}
