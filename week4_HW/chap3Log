                do_ls([m[1C.[1C[31m);
        }
        else{
                for(i=0;i<ac; i++)
                {
                //      printf([m[1C%s:\n[1C[31m, av[i]);
                        do_ls(av[i]);
                //      printf([m[1C\n[1C[31m);
                }
        }
        return 0;
}//end main

void do_ls(const char dirname[]){
        //list files in directory <dirname>
        DIR *dir_ptr; /*the directory*/
        struct dirent *direntp; //each entry. dirent has d_name, d_type, d_reclen
        
        if((dir_ptr = opendir(dirname))==NULL){
                fprintf(stderr, [m[1Cls1: cannot open %s\n[1C[31m, dirname);
                return;
        }
        
        while(( direntp=readdir(dir_ptr))!=NULL){
                if((direntp -> d_name[0])== '.'){
                        continue;
                }
                printf([m[1C%s\n[1C[31m, direntp->d_name);
        }
        closedir(dir_ptr);
        
        
}[m[63;224H6-23[11;23H[?25h[?25l[31m%[m[11;22H[K[63;224H5-22[11;22H[?25h[?25l[11;21H[K[63;224H4-21[11;21H[?25h[?25l[11;20H[K[63;224H3-20[11;20H[?25h[?25l[11;19H[K[63;224H2-19[11;19H[?25h[?25l[11;18H[K[63;224H1-18[11;18H[?25h[?25l[11;17H[K[63;224H0-17[11;17H[?25h[?25l[11;16H[K[12;1H        [33mif[m(ac==[1C)[1m[37m[41m{[m
                do_ls([1C[31m.[m[1C);
        [1m[37m[41m}[m
        [33melse[m[1m[37m[41m{[m
                [33mfor[m(i=[1C;i<ac; i++)
                [1m[37m[41m{[m
                [34m//      printf([m[1C[31m%s:[m[35m\n[m[1C[34m, av[i]);[m
                        do_ls(av[i]);
                [34m//      printf([m[1C[35m\n[m[1C[34m);[m
                [1m[37m[41m}[m
        }
        [33mreturn[m [1C;
}[34m//end main[m

