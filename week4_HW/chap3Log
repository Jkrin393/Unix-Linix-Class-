void do_ls(const char dirname[]){
        //list files in directory <dirname>
        DIR *dir_ptr; /*the directory*/
        struct dirent *direntp; //each entry. dirent has d_name, d_type, d_reclen
        
        if((dir_ptr = opendir(dirname))==NULL){
                fprintf(stderr, [m[1Cls1: cannot open %s\n[1C[31m, dirname);
                return;
        }
        
        while(( direntp=readdir(dir_ptr))!=NULL){
                if((direntp -> d_name[0])== '.'){
                        continue;
                }
                printf([m[1C%s\n[1C[31m, direntp->d_name);
        }
        closedir(dir_ptr);
        
        
}[m[63;224H1-25[13;25H[?25h[?25l[31ma[m[63;224H2-26[13;26H[?25h[?25l[31mc[m[63;224H3-27[13;27H[?25h[?25l[31m [m[63;224H4-28[13;28H[?25h[?25l[31m=[m[63;224H5-29[13;29H[?25h[?25l[31m [m[63;224H6-30[13;30H[?25h[?25l[31m1[m[63;224H7-31[13;31H[?25h[?25l[31m [m[63;224H8-32[13;32H[?25h[?25l[35m\[m[63;224H9-33[13;33H[?25h[?25l[35mm[m[63;223H20-34[13;34H[?25h[?25l[31m"[m
        }
        [33melse[m{
                [33mfor[m(i=[1C;i<ac; i++)
                {
                [34m//      printf([m[1C[31m%s:[m[35m\n[m[1C[34m, av[i]);[m
                        do_ls(av[i]);
                [34m//      printf([m[1C[35m\n[m[1C[34m);[m
                }
        [34m//}[m
        [33mreturn[m [1C;
}[34m//end main[m

