[32mint[m main([32mint[m ac, [32mchar[m *av[]){[10;9H[32mint[m i=[31m0[m;[11;9H[33mif[m(ac==[31m1[m){[12;17Hdo_ls([31m"."[m);[13;9H}[14;9H[33melse[m{[15;17H[33mwhile[m(i<ac){[16;25Hprintf([31m"[m[35m%s[m[31m:[m[35m\n[m[31m"[m, *++av);[17;25Hdo_ls(av[i]);[18;17H}[19;9H}
