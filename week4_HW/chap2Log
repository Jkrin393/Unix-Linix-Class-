                if(write(out_fd, buf, n_chars) !=n_chars){
                        oops([m[1Cwrite error to [1C[31m, av[2]);
                }
        }
        if(n_chars == -1){
                oops([m[1Cread error from [1C[31m, av[1]);
        }
        /*close files*/
        if(close(in_fd) == -1 || close(out_fd) == -1){
                oops([m[1Cerror closing files: [1C[31m,"");
        }
        return 0;
}

void oops(char *s1, char *s2){
        fprintf(stderr, [m[1CError %s [1C[31m, s1);
        perror(s2);
        exit(1);
}[m[63;224H7-41[33;41H[?25h[?25l[31m" || owrite_input == "[mN[31m"){[m[33;66H[K[63;224H6-40[33;40H[?25h[?25l[31m" || owrite_input == "[mN[31m"){[m[33;65H[K[63;224H5-39[33;39H[?25h[?25l[31m" || owrite_input == "[mN[31m"){[m[33;64H[K[63;224H4-38[33;38H[?25h[63;1H[K[33;37H[?25l[63;210H^[[33;37H[63;210H  [33;38H[63;220H33,23-37      All[33;37H[?25h[?25l[63;210Hu[33;37H[63;1H1 change; before #17  6 seconds ago[63;210H[K[33;42H== [31m"n"[m || owrite_input == [31m"N"[m){
                                
                }
        }


        [33mif[m((out_fd=creat(av[[1C], COPYMODE)) == -[1C){
                oops([1C[31mCannot creat[m[1C, av[[1C]);
        }
        [34m/*copy files */[m
        [33mwhile[m((n_chars=read(in_fd, buf, BUFFERSIZE)) > [1C) {
