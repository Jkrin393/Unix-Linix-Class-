}[63;224H0-64[33;64H[?25h[?25l[63;223H49-63[33;63H[?25h[?25l[33;62H[K[63;224H8-62[33;62H[?25h[?25l[33;61H[K[63;224H7-61[33;61H[?25h[?25l[63;224H6-60[33;60H[?25h[?25l[33;59H[K[63;224H5-59[33;59H[?25h[?25l[33;58H[K[63;224H4-58[33;58H[?25h[?25l[33;57H[K[63;224H3-57[33;57H[?25h[?25l[33;56H[K[63;224H2-56[33;56H[?25h[?25l[33;55H[K[63;224H1-55[33;55H[?25h[?25l[33;54H[K[63;224H0-54[33;54H[?25h[?25l[33;53H[K[63;223H39-53[33;53H[?25h[?25l[33;52H[K[63;224H8-52[33;52H[?25h[?25l[33;51H[K[63;224H7-51[33;51H[?25h[?25l[33;50H[K[63;224H6-50[33;50H[?25h[?25l[33;49H[K[63;224H5-49[33;49H[?25h[?25l[33;48H[K[63;224H4-48[33;48H[?25h[?25l[63;224H3-47[33;47H[?25h[?25l[33;46H[K[63;224H2-46[33;46H[?25h[?25l[33;45H[K[63;224H1-45[33;45H[?25h[?25l[63;224H0-44[33;44H[?25h[?25l[33;43H[K[34;1H[31m        }


        if((out_fd=creat(av[2], COPYMODE)) == -1){
                oops([m[1CCannot creat[1C[31m, av[2]);
        }
        /*copy files */
        while((n_chars=read(in_fd, buf, BUFFERSIZE)) > 0) {
                if(write(out_fd, buf, n_chars) !=n_chars){
                        oops([m[1Cwrite error to [1C[31m, av[2]);
                }
        }
        if(n_chars == -1){
                oops([m[1Cread error from [1C[31m, av[1]);
        }
        /*close files*/
        if(close(in_fd) == -1 || close(out_fd) == -1){
                oops([m[1Cerror closing files: [1C[31m,"");
        }
        return 0;
}

