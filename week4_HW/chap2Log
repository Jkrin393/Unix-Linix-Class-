        perror(s2);
        exit([1C);
}[29;38H[?25h[?25l[31m'n'[m || owrite_input == [31m"N"[m){[63;224H5-39[29;39H[?25h[?25l[63;224H6-40[29;40H[?25h[?25l[63;224H7-41[29;41H[?25h[?25l[63;224H8-42[29;42H[?25h[?25l[63;224H9-43[29;43H[?25h[?25l[63;223H30-44[29;44H[?25h[?25l[63;224H1-45[29;45H[?25h[?25l[63;224H2-46[29;46H[?25h[?25l[63;224H3-47[29;47H[?25h[?25l[63;224H4-48[29;48H[?25h[?25l[63;224H5-49[29;49H[?25h[?25l[63;224H6-50[29;50H[?25h[?25l[63;224H7-51[29;51H[?25h[?25l[63;224H8-52[29;52H[?25h[?25l[63;224H9-53[29;53H[?25h[?25l[63;223H40-54[29;54H[?25h[?25l[63;224H1-55[29;55H[?25h[?25l[63;224H2-56[29;56H[?25h[?25l[63;224H3-57[29;57H[?25h[?25l[63;224H4-58[29;58H[?25h[?25l[63;224H5-59[29;59H[?25h[?25lN[31m"){[m[29;63H[K[30;1H[31m                        printf([m[1CFile not copied, program ending.[1C[31m);
                        exit(1);
                }
                
        }


        if((out_fd=creat(av[2], COPYMODE)) == -1){
                oops([m[1CCannot creat[1C[31m, av[2]);
        }
        /*copy files */
        while((n_chars=read(in_fd, buf, BUFFERSIZE)) > 0) {
                if(write(out_fd, buf, n_chars) !=n_chars){
                        oops([m[1Cwrite error to [1C[31m, av[2]);
                }
        }
        if(n_chars == -1){
                oops([m[1Cread error from [1C[31m, av[1]);
        }
        /*close files*/
        if(close(in_fd) == -1 || close(out_fd) == -1){
                oops([m[1Cerror closing files: [1C[31m,"");
        }
        return 0;
}

void oops(char *s1, char *s2){
        fprintf(stderr, [m[1CError %s [1C[31m, s1);
        perror(s2);
        exit(1);
