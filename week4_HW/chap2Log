[1;63r[35;25Hexit([31m1[m);[62;1H}[63;220H[K[63;220H35,12-33      All[35;33H[?25h[?25l[35;32H[K[35;29H[46m([1C)[m[63;224H1-32[35;32H[?25h[?25l[35;31H[K[35;29H([63;224H0-31[35;31H[?25h[?25l[35;30H[K[63;223H9-30 [35;30H[?25h[?25l[35;29H[K[63;223H8-29[35;29H[?25h[?25l[35;28H[K[63;223H7-28[35;28H[?25h[?25l[35;27H[K[63;223H6-27[35;27H[?25h[?25l[35;26H[K[63;223H5-26[35;26H[?25h[?25l[35;25H[K[63;223H4-25[35;25H[?25h[?25l[63;223H3-17[35;17H[?25h[?25l[63;223H2-9 [35;9H[?25h[?25l[63;223H1  [35;1H[?25h[?25l[34;62r[62;1H
[1;63r[34;25Hprintf([31m"please enter a valid choice."[m);[62;1H[1m[34m~                                                                                                                                                                                                                                            [m[63;220H[K[63;220H34,43-64      All[34;64H[?25h[?25l[34;63H[K[34;31H[46m([30C)[m[63;224H2-63[34;63H[?25h[?25l[34;62H[K[34;31H([63;224H1-62[34;62H[?25h[?25l[34;61H[K[35;1H[31m        }


        if((out_fd=creat(av[2], COPYMODE)) == -1){
                oops([m[1CCannot creat[1C[31m, av[2]);
        }
        /*copy files */
        while((n_chars=read(in_fd, buf, BUFFERSIZE)) > 0) {
                if(write(out_fd, buf, n_chars) !=n_chars){
                        oops([m[1Cwrite error to [1C[31m, av[2]);
                }
        }
        if(n_chars == -1){
                oops([m[1Cread error from [1C[31m, av[1]);
        }
        /*close files*/
        if(close(in_fd) == -1 || close(out_fd) == -1){
                oops([m[1Cerror closing files: [1C[31m,"");
        }
        return 0;
}

