[32mint[m main([32mint[m av, [32mchar[m* args[]){[16;9H[32mconst[m [32mchar[m* old = args[[31m1[m];[17;9H[32mconst[m [32mchar[m* new = args[[31m2[m];[19;9H[33mif[m(av <[31m2[m){[20;17Hprintf([31m"not enough arguments[m[35m\n[m[31m"[m);[21;17Hexit([31m1[m);[22;9H}[24;9H[33mif[m(old==[31mNULL[m || new == [31mNULL[m){[25;17Hprintf([31m"invalid file path[m[35m\n[m[31m"[m);[26;17Hexit([31m1[m);[27;9H}[30;9Hmove_file(old, new);

