int is_directory(char* argument){
        struct stat stat_path;

        //checks with stat if the path is a directory. Error will occur if it is a file
        if(stat(argument, &stat_path) != 0){
                return 0;// 0 here used to represent false
        }
        return 1;
}[m[63;224H1-25[43;25H[?25h[?25l[31mn[m[63;224H2-26[43;26H[?25h[?25l[31me[m[63;224H3-27[43;27H[?25h[?25l[31mw[m[63;224H4-28[43;28H[?25h[?25l[43;27H[K[63;224H3-27[43;27H[?25h[?25l[43;26H[K[63;224H2-26[43;26H[?25h[?25l[43;25H[K[63;224H1-25[43;25H[?25h[?25l[31mn[m[63;224H2-26[43;26H[?25h[?25l[31me[m[63;224H3-27[43;27H[?25h[?25l[31mw[m[63;224H4-28[43;28H[?25h[?25l[43;27H[K[63;224H3-27[43;27H[?25h[?25l[43;26H[K[63;224H2-26[43;26H[?25h[?25l[43;25H[K[63;224H1-25[43;25H[?25h[?25l[43;24H[K[44;1H        }
