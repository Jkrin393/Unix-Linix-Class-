[32mvoid[m move_file([32mconst[m [32mchar[m* old_path, [32mchar[m* new_path){[32;9H[34m//check if destination is a directory[m[33;9H[32mstruct[m stat stat_path;[34;9Hprintf([31m"stat: [m[35m%d\n[m[31m "[m,stat(new_path, &stat_path));[35;9Hprintf([31m"IsDIR: [m[35m%d\n[m[31m "[m,S_ISDIR(stat_path.st_mode));[37;9H[33mif[m[46m([mstat(new_path, &stat_path) != [31m0[m && S_ISDIR(stat_path.st_mode)[46m)[m{[34m//check if the path exists & is valid by calling stat. Checks if it is a directory with S_ISDIR from stat.h [m[39;17H[32mchar[m* dir_file_name;[40;17Hdir_file_name = strrchr(old_path, [31m'/'[m);[42;17H[34m//create new file path[m[43;17H[32msize_t[m new_path_len = strlen(new_path);[44;17H[32msize_t[m dir_file_name_len = strlen(dir_file_name);[45;17H[32mchar[m* new_dir_path = malloc(new_path_len + dir_file_name_len + [31m2[m);[47;17H[33mif[m(new_dir_path == [31mNULL[m){[48;25Hprintf([31m"no memory available"[m);[49;25Hexit([31m1[m);[50;17H}[52;17Hstrcpy(new_dir_path, new_path);[53;17Hstrcat(new_dir_path, [31m"/"[m);[54;17Hstrcat(new_dir_path, dir_file_name);[56;17Hprintf([31m"New Path [m[35m%s\n[m[31m"[m,new_dir_path);[60;9H}

