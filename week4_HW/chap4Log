[32mint[m move_file([32mconst[m [32mchar[m* old_path, [32mchar[m* new_path){[25;9H[32mint[m isDir;[26;9HisDir = is_directory(new_path);[30;9H[33mif[m(is_directory(new_path) == [31m0[m){[34m//second argument is a file[m[31;17Hrename(old_path, new_path);[32;9H}[33;9H[33melse[m{   [34m//destination is a directory [m[31m"mv test.txt /data_files/etc"[m[34;17H[34m//create new path location with old_path's name[m[35;17H[32mchar[m* new_dir_filename;[36;17Hnew_dir_filename = strrchr(new_path, [31m'/'[m);      [34m//strrchr returns a pointer to the last instance of the character[m[38;17H[32mchar[m new_dir_name[strlen(new_path) + strlen(new_dir_filename)+[31m2[m]; [34m// +[m[31m2[m[34m to allow for backslash &  null pointer[m[40;17Hstrcpy(new_dir_name, new_path);[41;17Hstrcat(new_dir_name, [31m"/"[m);[42;17Hstrcat(new_dir_name, new_dir_filename + [31m1[m);[44;17Hprintf([31m"[m[35m%s\n[m[31m"[m, new_dir_name);[46;17H[33mif[m(rename(old_path, new_path) !=[31m0[m){[47;25Hprintf([31m"Error renaming file[m[35m\n[m[31m"[m);[48;17H}[49;9H}

}


