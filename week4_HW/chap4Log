[32mint[m move_file([32mconst[m [32mchar[m* old_path, [32mchar[m* new_path){[37;9H[32mint[m isDir;[38;9HisDir = is_directory(new_path);[39;9H[32mchar[m* new_dir_filename;[40;9H[32mchar[m new_dir_name[strlen(new_path) + strlen(new_dir_filename)+[31m2[m]; [34m// +[m[31m2[m[34m to allow for backslash &  null pointer[m[44;9H[33mif[m(is_directory(new_path) == [31m0[m){[34m//second argument is a file[m[45;17Hrename(old_path, new_path);[46;9H}[47;9H[33melse[m{   [34m//destination is a directory [m[31m"mv test.txt /data_files/etc"[m[48;17H[34m//create new path location with old_path's name[m[49;17Hnew_dir_filename = strrchr(new_path, [31m'/'[m);      [34m//strrchr pulls characters after final instance of 2nd argument[m[51;17Hstrcpy(new_dir_name, new_path);[52;17Hstrcat(new_dir_name, [31m"/"[m);[53;17Hstrcat(new_dir_name, new_dir_filename + [31m1[m);[55;17Hprintf(new_dir_name);[57;17H[33mif[m(rename(old_path, new_dir_name) !=[31m0[m){[58;25Hprintf([31m"Error renaming file[m[35m\n[m[31m"[m);[59;17H}[60;9H}

