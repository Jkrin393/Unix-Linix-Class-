[33m 28 [m[12C    exit([31m1[m);
[33m 29 [m        }
[33m 30 
 31 
 32 [m        move_file(old, new);
[33m 33 
 34 [m}
[33m 35 
 36 [m[32mint[m move_file([32mconst[m [32mchar[m* old_path, [32mchar[m* new_path){
[33m 37 
 38 [m        [32mint[m isDir;
[33m 39 [m        isDir = is_directory(new_path);
[33m 40 
 41 
 42 
 43 [m        [33mif[m(is_directory(new_path) == [31m0[m){[34m//file is not a directory[m
[33m 44 [m[12C    new_dir_path = strrchr(old_path, [31m'/'[m);  [34m//strrchr pulls characters after final instance of 2nd argument[m
[33m 45 [m[12C    printf(new_dir_path);
[33m 46 
 47 [m        }
[33m 48 [m        [33melse[m{   [34m//destination is a file, use rename[m
[33m 49 [m[12C    [33mif[m(rename(old_path, new_path) !=[31m0[m){
[33m 50 [m[20C    printf([31m"Error renaming file[m[35m\n[m[31m"[m);
[33m 51 [m[12C    }
[33m 52 [m        }
[33m 53 
 54 [m}
[33m 55 
 56 
