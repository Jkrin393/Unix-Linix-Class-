                printf(new_dir_path);
        
                if(rename(old_path, new_path) !=0)){
                        printf([m[1CError renaming file\n[1C[31m);
                }
        }

}


int is_directory(char* argument){
        struct stat stat_path;

        //checks with stat if the path is a directory. Error will occur if it is a file
        if(stat(argument, &stat_path) != 0){[46;38H"[m[31m[46m)[m[63;224H5-39[46;39H[?25h[?25l[31m[46m/[m[31m);/[m[31m[46m)[m[63;224H6-40[46;40H[?25h[?25l[31m[46m"[m);

                printf(new_dir_path);
        
                [33mif[m(rename(old_path, new_path) !=[1C)[1m[37m[41m)[m{
                        printf([1C[31mError renaming file[m[35m\n[m[1C);
                }
        }

}


