[32mint[m main([32mint[m av, [32mchar[m* args[]){[17;9H[32mconst[m [32mchar[m* old = args[[31m1[m];[18;9H[32mchar[m* new = args[[31m2[m];[20;9H[33mif[m(av !=[31m3[m){[21;17Hprintf([31m"not enough arguments[m[35m\n[m[31m"[m);[22;17Hexit([31m1[m);[23;9H}[25;9Hmove_file(old, new);[26;9H[33mreturn[m [31m0[m;
