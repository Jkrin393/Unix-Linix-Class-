[32mvoid[m move_file([32mconst[m [32mchar[m* old_path, [32mchar[m* new_path){[32;9H[34m//if destination is a directory[m[33;9H[32mstruct[m stat stat_path;[35;9H[33mif[m((stat(new_path, &stat_path) == [31m0[m) &&( S_ISDIR(stat_path.st_mode) !=[31m0[m)){[34m//check if the path exists & is valid by calling stat. Checks if it is a directory with S_ISDIR from stat.h. non-zero when directory [38;17H//create new file path[m[39;17H[32msize_t[m old_len = strlen(old_path);[40;17H[32msize_t[m new_len = strlen(new_path);[41;9H[34m//      printf([m[31m"dir len: %ld, path_len: %ld"[m[34m, old_len, new_len); [m[43;17H[32mchar[m* new_dir_path = malloc(old_len + new_len + [31m2[m);[45;17H[33mif[m(new_dir_path == [31mNULL[m){[46;25Hprintf([31m"no memory available"[m);[47;25Hexit([31m1[m);[48;17H}[50;17Hstrcpy(new_dir_path, new_path);[51;17Hstrcat(new_dir_path, [31m"/"[m);[52;17Hstrcat(new_dir_path, old_path);[54;17H[34m//create new link[m[55;17H[33mif[m(link(old_path, new_dir_path) == -[31m1[m){[56;25Hperror([31m"Error creating new link"[m);[57;25Hfree(new_dir_path);[58;25Hexit([31m1[m);[59;17H}[61;17H[34m//remove the link from the old_path[m[62;17H[33mif[m(unlink(old_path)==-[31m1[m){[63;218H41,1-8[8CTop[41;8H[?25h[?1000l[?1002h[?25l[63;208H~@k[41;8H[63;208H   [42;1H[63;219H2,0-1[42;1H[?25h[?25l[63;219H3,1-8[43;8H[?25h[?25l[63;208H~@k[43;8H[63;208H   [44;1H[63;219H4,0-1[44;1H[?25h[?25l[63;208H~@k[44;1H[63;208H   [45;8H[63;219H5,1-8[45;8H[?25h[?25l[63;208H~@k[45;8H[63;208H   [46;8H[63;219H6[46;8H[?25h[?25l[63;208H~@k[46;8H[63;208H   [47;8H[63;219H7[47;8H[?25h[?25l[63;208H~@k[47;8H[63;208H   [48;8H[63;219H8[48;8H[?25h[?25l[63;208H~@k[48;8H[63;208H   [49;1H[63;219H9,0-1[49;1H[?25h[?25l[63;208H~@k[49;1H[63;208H   [50;8H[63;218H50,1-8[50;8H[?25h[?25l[63;208H~@k[50;8H[63;208H   [51;8H[63;219H1[51;8H[?25h[?25l[63;208H~@k[51;