[34m//function to move file[m
[32mvoid[m move_file([32mconst[m [32mchar[m* old_path, [32mchar[m* new_path);

[34m//function to detect if second argument is a directory[m
[32mint[m is_directory([32mchar[m* argument);


[32mint[m main([32mint[m av, [32mchar[m* args[]){[18;9H[32mconst[m [32mchar[m* old = args[[31m1[m];[19;9H[32mchar[m* new = args[[31m2[m];[21;9H[33mif[m(av <[31m3[m){[22;17Hprintf([31m"not enough arguments[m[35m\n[m[31m"[m);[23;17Hexit([31m1[m);[24;9H}[26;9H[33mif[m(old==[31mNULL[m || new == [31mNULL[m){[27;17Hprintf([31m"invalid file path[m[35m\n[m[31m"[m);[28;17Hexit([31m1[m);[29;9H}[32;9Hmove_file(old, new);

}

[32mvoid[m move_file([32mconst[m [32mchar[m* old_path, [32mchar[m* new_path){[39;9H[33mif[m(is_directory(new_path) == [31m0[m){[34m//second argument is a file[m[40;17Hprintf([31m"entered if block of move_file[m[35m\n[m[31m"[m);[41;17Hrename(old_path, new_path);[42;9H}[43;9H[33melse[m{   [34m//destination is a directory [m[31m"mv test.txt /data_files/etc"[m[44;17H[34m//create new path location with old_path's name[m[45;17Hprintf([31m"entered else block[m[35m\n[m[31m"[m);[46;17H[32mchar[m* new_dir_filename;[47;17Hnew_dir_filename = strrchr(new_path, [31m'/'[m);      [34m//strrchr returns a pointer to the last instance of the character[m[48;17Hprintf([31m"New Name: [m[35m%s\n[m[31m"[m, new_dir_filename);[49;17H[32mchar[m new_dir_name[(strlen(new_path) + strlen[46m([mnew_dir_filename[46m)[m)+[31m2[m]; [34m// +[m[31m2[m[34m to allow for backslash &  null pointer[m[51;17Hstrcpy(new_dir_name, new_path);[52;17Hstrcat(new_dir_name, [31m"/"[m);[53;17Hstrcat(new_dir_name, old_path + [31m1[m);[55;17Hprintf([31m"[m[35m%s\n[m[31m"[m, new_dir_name);[57;17H[33mif[m(rename(old_path, new_dir_name) !=[31m0[m){[58;25Hprintf([31m"Error renaming file[m[35m\n[m[31m"[m);[59;17H}[60;9H}

