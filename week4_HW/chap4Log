[34m//function to move file[m
[32mint[m move_file([32mconst[m [32mchar[m* old_path, [32mchar[m* new_path);

[34m//function to detect if second argument is a directory[m
[32mint[m is_directory([32mchar[m* argument);


[32mint[m main([32mint[m av, [32mchar[m* args[]){[18;9H[32mconst[m [32mchar[m* old = args[[31m1[m];[19;9H[32mchar[m* new = args[[31m2[m];[21;9H[33mif[m(av <[31m2[m){[22;17Hprintf([31m"not enough arguments[m[35m\n[m[31m"[m);[23;17Hexit([31m1[m);[24;9H}[26;9H[33mif[m(old==[31mNULL[m || new == [31mNULL[m){[27;17Hprintf([31m"invalid file path[m[35m\n[m[31m"[m);[28;17Hexit([31m1[m);[29;9H}[32;9Hmove_file(old, new);

}

[32mint[m move_file([32mconst[m [32mchar[m* old_path, [32mchar[m* new_path){[38;9H[32mint[m isDir;[39;9HisDir = is_directory(new_path);[40;9H[32mchar[m* new_dir_path;[43;9H[33mif[m(is_directory(new_path) == [31m0[m){[34m//file is not a directory[m[44;17Hnew_dir_path = strrchr(old_path, [31m'/'[m);  [34m//strrchr pulls characters after final instance of 2nd argument[m[45;17Hprintf(new_dir_path);[47;9H}[48;9H[33melse[m{   [34m//destination is a file, use rename[m[49;17H[33mif[m(rename(old_path, new_path) !=[31m0[m){[50;25Hprintf([31m"Error renaming file[m[35m\n[m[31m"[m);[51;17H}[52;9H}

}


[32mint[m is_directory([32mchar[m* argument){[58;9H[32mstruct[m stat stat_path;[60;9H[34m//checks with stat if the path is a directory. Error will occur if it is a file[m[61;9H[33mif[m(stat(argument, &stat_path) != [31m0[m){[62;17H[33mreturn[m [31m0[m;[34m// [m[31m0[m[34m here used to represent false[m[63;220H19,2-9[8CTop[19;9H[?25h[?1000l[?1002h[?25l[63;210Hl[19;9H[63;210H [19;10H[63;223H3-10[19;10H[?25h[?25l[63;210Hs[19;10H[63;210H [19;10H[63;210Hcl[19;10H[63;210H  [19;10H[63;1H[1m-- INSERT --[m[63;13H[K[63;220H19,3-10[7CTop[19;9Hcar* new = args[[31m2[m];[19;28H[K[19;10H[?25h[?25lar* new = args[[31m2[m];[19;27H[K[63;223H2-9 [19;9H[?25h[?25lar* new = args[[31m2[m];[19;19H[K[63;223H1  [19;1H[?25h[63;1H[K[19;1H[?25l[63;210H^[[19;1H[63;210H  [19;1H[63;220H19,1[10CTop[19;1H[?25h[?25l[63;210Hu[19;1H[63;1H1 change; before #1  1 second ago[63;210H[K[19;10H        [32mchar[m* new = args[[31m2[m];[63;220H19,3-10[7CTop[63;220H[K[63;220H19,3-10[7CTop[19;10H[?25h[?25l[63;210H~@k[19;10H[63;210H   [20;1H[63;220H20,0-1 [20;1H[?25h[?25l[63;210H~@k[20;1H[63;210H   [21;10H[63;221H1,3-10[21;10H[?25h[?25l[63;210H~@k[21;10H[63;210H   [22;16H[63;221H2,2-16[22;16H[?25h[?25l[63;210H~@k[22;16H[63;210H   [23;16H[63;221H3[23;16H[?25h[?25l[63;210H~@k[23;16H[63;210H   [24;9H[21;18H[46m{[24;9H}[m[63;221H4,2-9 [24;9H[?25h[?25l[63;210H~@k[24;9H[63;210H   [25;1H[21;18H{[24;9H}[63;221H5,0-1[25;1H[?25h[?25l[63;210H~@k[25;1H[63;210H   [26;10H[63;221H6,3-10[26;10H[?25h[?25l[63;210H~@k[26;10H[63;210H   [27;16H[63;221H7,2-16[27;16H[?25h[?25l[63;210H~@k[27;16H[63;210H   [28;16H[63;221H8[28;16H[?25h[?25l[63;210H~@k[28;16H[63;210H   [29;9H[26;37H[46m{[29;9H}[m[63;221H9,2-9 [29;9H[?25h[?25l[63;210H~@k[29;9H[63;210H   [30;8H[26;37H{[29;9H}[63;220H30,1-8[30;8H[?25h[?25l[63;210H~@k[30;8H[63;210H  