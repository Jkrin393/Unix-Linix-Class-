}

[32mint[m move_file([32mconst[m [32mchar[m* old_path, [32mchar[m* new_path){[38;9H[32mint[m isDir;[39;9HisDir = is_directory(new_path);[43;9H[33mif[m(is_directory(new_path) == [31m0[m){[34m//second argument is a file[m[44;17Hrename(old_path, new_path);[45;9H}[46;9H[33melse[m{   [34m//destination is a directory [m[31m"mv test.txt /data_files/etc"[m[47;17H[34m//create new path location with old_path's name[m[48;17H[32mchar[m* new_dir_filename;[49;17Hnew_dir_filename = strrchr(new_path, [31m'/'[m);      [34m//strrchr pulls characters after final instance of 2nd argument[m[51;17H[32mchar[m new_dir_name[strlen(new_path) + strlen(new_dir_filename)+[31m2[m]; [34m// +[m[31m2[m[34m to allow for backslash &  null pointer[m[53;17Hstrcpy(new_dir_name, new_path);[54;17Hstrcat(new_dir_name, [31m"/"[m);[55;17Hstrcat(new_dir_name, new_dir_filename + [31m1[m);[57;17Hprintf(new_dir_name);[59;17H[33mif[m(rename(old_path, new_dir_name) !=[31m0[m){[60;25Hprintf([31m"Error renaming file[m[35m\n[m[31m"[m);[61;17H}[62;9H}[63;220H41,0-1[8CTop[41;1H[?25h[?1000l[?1002h[?25l[63;210H~@k[41;1H[63;210H   [42;8H[63;221H2,1-8[42;8H[?25h[?25l[63;210H~@k[42;8H[63;210H   [41;1H[63;221H1,0-1[41;1H[?25h[?25l[63;210H~@k[41;1H[63;210H   [42;8H[63;221H2,1-8[42;8H[?25h[?25l[63;210H~@k[42;8H[63;210H   [43;8H[63;221H3[43;8H[?25h[?25l[63;210H~@k[43;8H[63;210H   [44;8H[63;221H4[44;8H[?25h[?25l[63;210H~@k[44;8H[63;210H   [45;8H[63;221H5[45;8H[?25h[?25l[63;210H~@k[45;8H[63;210H   [46;8H[63;221H6[46;8H[?25h[?25l[63;210H~@k[46;8H[63;210H   [47;8H[63;221H7[47;8H[?25h[?25l[63;210H~@k[47;8H[63;210H   [48;8H[63;221H8[48;8H[?25h[?25l[63;210H~@k[48;8H[63;210H   [49;8H[63;221H9[49;8H[?25h[?25l[63;210H~@k[49;8H[63;210H   [49;16H[63;223H2-16[49;16H[?25h[?25l[63;210Hi[49;16H[63;210H [49;9H[63;1H[1m-- INSERT --[m[63;13H[K[63;220H49,2-9[8CTop[49;9H[?